package Aula2;

public class Familia {
	String[] nomes = {"Rafael","Eliene","Matheus"};
double[] idade= {38,37,3};
String[] sexo2={"M","f"};
String endereco;
 String pai;
 String filho;
 String Esposa;
void transcreva() {
	System.out.println("Pai-"+this.nomes[0]+"-Sexo-"+this.sexo2[0]+"idade"+this.idade[0]+
			"\nEsposa-"+this.nomes[1]+"-Sexo-"+this.sexo2[1]+"idade"+this.idade[1]+
			"\nFilho-"+this.nomes[2]+"-Sexo-"+this.sexo2[0]+"idade"+this.idade[2]
			);

}

}
